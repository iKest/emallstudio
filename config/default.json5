{
    production: false,
    app: {
        // Путь к папке с билдом
        distFolder: './public_html',
        // Ширина игры
        GAME_WIDTH: 900,
        //  Высота игры
        GAME_HEIGHT: 1600,
        // Адрес хостинга приложения (для дев. режима значение порта совпадает со значением порта дев. сервера PORT)
        APP_URL: 'http://localhost:8080/index.html',
        // Путь к полифиллу
        polyfill: 'https://polyfill.io/v3/polyfill.js?features=es5,es6,es7&flags=gated',
        // Путь к дистрибутиву Phaser
        external: '/js/external.js',
        // Путь к основному скрипту игры
        bundle: '/js/bundle.js',
        // Путь к ресурсам игры
        ASSETS_PATH: 'assets',
        // Порт дев. сервера (browserSync)
        PORT: 8080,
        // Порт интерфейса дев. сервера (browserSync)
        UI_PORT: 8081
    },
    // Данные для рендера HTML страниц
    html_data: {
        // цвет адресной строки и строки уведомлений браузера
        themeColor: '#ffffff',
        // цвет при наведении на иконку для закреплённых вкладок Safari и кнопок на тач-баре.
        maskColor: '#5bbad5',
        // Цвет фона иконки для windows
        tileColor: '#da532c',
        // Каноническая ссылка на сайт с игрой
        canonical: 'http://localhost:8080',
        // Заголовок страницы с игрой
        title: 'Best Coffe for Friends',
        // Короткое имя для манифеста
        shortName: 'BestCoffe',
        // Описание страницы с игрой
        description: 'Дарим лучший кофе для друзей',
        // URL к файлу конфигурации для MS IE и MS Edge
        msConfig: '/browserconfig.xml',
        facebook: {
            // заголовок страницы для Open Graph
            title: 'Best Coffe for Friends',
            // Тип побъекта на странице для Open Graph
            type: 'website',
            // Краткое описание страницы длиной не более 300 символов для Open Graph
            description: 'Дарим лучший кофе для друзей',
            // Адрес сайта для Open Graph
            url: 'http://localhost:8080',
            // URL изображения, характеризующего страницу для Open Graph
            image: 'http://localhost:8080/fb.png',
            // Название сайта для Open Graph
            siteName: 'Best Coffe for Friends',
            // Язык страницы для Open Graph
            locale: 'ru_RU',
            // id администатора приложения или группы приложения на FaceBook (необязательное значение)
            admins: 100004865342063
        },
        // Путь к лоадеру скриптов
        loader: '/js/loader.js',
        // Путь к стилям
        style: '/styles/style.css',
    }
}
