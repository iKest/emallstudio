{
    "extends": ["eslint:recommended", "google", "prettier", "prettier/babel"],
    "plugins": ["babel", "prettier"],
    "parser": "babel-eslint",
    "globals": {
        "GAME_HEIGHT": true,
        "GAME_WIDTH": true,
        "ASSETS_PATH": true,
        "GAME_CONTAINER": true,
        "EXTERNAL": true
    },
    "rules": {
        "new-cap": 0,
        "camelcase": 0,
        "no-invalid-this": 0,
        "object-curly-spacing": 0,
        "quotes": 0,
        "semi": 0,
        "no-unused-expressions": 0,
        "valid-typeof": 0,
        "babel/new-cap": 0,
        "babel/camelcase": 0,
        "babel/no-invalid-this": 1,
        "babel/object-curly-spacing": 1,
        "babel/quotes": 1,
        "babel/semi": 1,
        "babel/no-unused-expressions": 1,
        "babel/valid-typeof": 1
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "commonjs": true
    }
}
